    "/api/tenant/users": {
      "get": {
        "summary": "ユーザー一覧取得",
        "tags": [
          "Tenant"
        ],
        "description": "テナントに紐づくユーザーの一覧を取得します。全てのロールで閲覧可能です。",
        "security": [
          {
            "Bearer": []
          }
        ],
        "responses": {
          "200": {
            "description": "ユーザー一覧取得成功",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
