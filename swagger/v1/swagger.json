                      },
                      "email": {
                        "type": "string",
                        "description": "メールアドレス"
                      },
                      "role": {
                        "type": "string",
                        "enum": [
                          "owner",
                          "manager",
                          "staff",
                          "kitchen_staff",
                          "cashier"
                        ],
                        "description": "ロール"
                      },
                      "created_at": {
                        "type": "string",
                        "format": "date-time",
                        "description": "作成日時"
                      },
                      "updated_at": {
                        "type": "string",
                        "format": "date-time",
                        "description": "更新日時"
                      }
                    },
                    "required": [
                      "id",
                      "name",
                      "email",
                      "role",
                      "created_at",
                      "updated_at"
                    ]
